{
    "name": "@teatak/ui",
    "version": "1.2.24",
    "description": "teatak ui",
    "license": "MPL-2.0",
    "main": "lib/index.js",
    "module": "es/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/teatak/ui.git"
    },
    "scripts": {
        "clean:commonjs": "rimraf lib",
        "clean:product": "rimraf docs/*.js docs/*.css docs/*.txt docs/*.html",
        "clean:es": "rimraf es",
        "watch": "cross-env NODE_ENV=dev webpack --config webpack.config.js --mode=development -w",
        "build:product": "cross-env NODE_ENV=prod webpack --config webpack.config.js --mode=production --progress",
        "build:commonjs": "babel src/ --out-dir lib/ --plugins @babel/plugin-transform-modules-commonjs --copy-files --ignore **/__tests__,**/__mocks__",
        "build:es": "babel src/ --out-dir es/ --copy-files --ignore **/__tests__,**/__mocks__",
        "build": "npm-run-all clean:* --parallel build:*",
        "prepack": "npm version patch && npm run build"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.13.14",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.12",
        "@babel/preset-react": "^7.13.13",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@loadable/component": "^5.15.0",
        "@mui/icons-material": "^5.3.1",
        "async-validator": "^3.5.2",
        "babel-loader": "^8.2.2",
        "babel-plugin-import": "^1.13.3",
        "classnames": "^2.3.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.0",
        "html-webpack-plugin": "^5.3.1",
        "less": "^4.1.1",
        "less-loader": "^8.0.0",
        "mini-css-extract-plugin": "^1.4.0",
        "npm-run-all": "^4.1.5",
        "nprogress": "^0.2.0",
        "postcss-cssnext": "^3.1.0",
        "postcss-import": "^14.0.1",
        "postcss-loader": "^5.2.0",
        "postcss-nested": "^5.0.6",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-markdown": "^6.0.2",
        "react-redux": "^7.2.3",
        "react-router-dom": "^5.2.0",
        "react-syntax-highlighter": "^15.4.3",
        "react-transition-group": "^4.4.2",
        "redux-thunk": "^2.3.0",
        "remark-gfm": "^1.0.0",
        "rimraf": "^3.0.2",
        "style-loader": "^2.0.0",
        "webpack": "^5.28.0",
        "webpack-cli": "^4.6.0"
    },
    "peerDependencies": {
        "react": ">=16.9.0",
        "react-dom": ">=16.9.0"
    }
}
